      <bind nodeset="/S_registo_entrega/Gg1/foto_example" readonly="true()" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" type="string"/>
      <bind nodeset="/S_registo_entrega/Gg1/pessoa_foto" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" required="true()" type="binary"/>
      <bind nodeset="/S_registo_entrega/Gg1/id_foto" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" required="true()" type="binary"/>
      <bind nodeset="/S_registo_entrega/Gg1/id_foto_verso" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" required="true()" type="binary"/>
      <bind nodeset="/S_registo_entrega/Gg1/pessoa_assin" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" required="true()" type="binary"/>
      <bind nodeset="/S_registo_entrega/Gg1/recibo_image" relevant=" /S_registo_entrega/Gg1/id_titular_confirm ='sim'" required="true()" type="binary"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/S_registo_entrega/end" type="dateTime"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/S_registo_entrega/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat('Declaracoes','_', /S_registo_entrega/assoc_id ,'_', /S_registo_entrega/nupn )" nodeset="/S_registo_entrega/meta/instanceName" type="string"/>
