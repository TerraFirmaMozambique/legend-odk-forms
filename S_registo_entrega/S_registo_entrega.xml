      <bind nodeset="/S_registo_entrega/preloadfail" readonly="true()" relevant="string-length( /S_registo_entrega/resultado ) &lt; 1" type="string"/>
      <bind nodeset="/S_registo_entrega/preloadnote" readonly="true()" type="string"/>
