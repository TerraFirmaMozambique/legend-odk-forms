    </input>
    <select1 appearance="minimal" ref="/LEGEND_M_objeccoes_correcoes/prov_id">
      <label>Selecionar a província</label>
      <item>
        <label>Cabo Delgado</label>
        <value>P02</value>
      </item>
      <item>
